---
import { type PatchOptions } from "@/types/attributes";
import { buildElementProps } from "../utils";

const { ...attributes } = Astro.props as PatchOptions<"footer">;

const built = buildElementProps<"footer">({
  attributes
});
---

<footer {...built}>Footer</footer>

<style>
  footer {
    display: flex;
    gap: 1rem;
    margin-top: 2rem;
  }
</style>
